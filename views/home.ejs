      <script>
        var a=document.querySelector("h1");
        var h1=a.textContent;
        var s=h1.split("");
        var c="";
        // var hh=Math.floor(s.length()/2);
        s.forEach(function(ele,idx)
    {
        if(idx<3){
        c+=`<span class="aa" >${ele}</span>`
        }
        else{
            c+=`<span class="bb">${ele}</span>`
        }
     
    })



    
        a.innerHTML=c;
        // var tl = gsap.timeline();
        // tl.from("h1 span",
        //     {
                
