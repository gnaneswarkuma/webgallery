        //         duration:1,
        //         rotate:"100deg",
        //         stagger:0.3,
        //     }
        // )

        var tl = gsap.timeline()
        var dd=document.querySelector(".full i");
        var d=document.querySelector(".nav i");
        tl.to(".full",{
            right:0,
            // opacity:0,
    scale:1,
            duration:0.8,
        })
        tl.from(".full h3",{
            stagger:0.4,
            duration:0.8,
            x:100,
            opacity:0,
            ease:"elastic.out",
        })
        tl.pause();
        d.addEventListener("click",function()
    {
       tl.play();
    })
    dd.addEventListener("click",function()
    {
       tl.reverse();
    })
